<?xml version="1.0" encoding="UTF-8"?>
<UITableViewCell
    >
    <RelativeLayout id="main"
        width="parent"
        height="parent"
        margin_left="10"
        >
        <UILabel
            id="title_label"
            text="xiaominfc"
            text_color="#0000ff"
            font="18"
            margin_left="8"
            right_of="header_image"
            center_h_parent="true"
            offset_h="-10"
            />
        
        <UILabel
            text="hello world"
            font="14"
            margin_top="8"
            left_of_align="title_label"
            bottom_of="title_label"
            />
        
        <UIImageView
            corner="5"
            id="header_image"
            width="60"
            height="60"
            src="header"
            center_h_parent="true"
            left_of_align="parent"
            margin_left="10"
            />
        
        <UIView
            width="parent"
            height="0.5"
            bottom_of_align="parent"
            center_v_parent="true"
            background_color="#11000000"
            />
    </RelativeLayout>
</UITableViewCell>
